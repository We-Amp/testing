[
  {
    "TestName": "FCGI Server and Client POST",
    "Description": "Test fcgi GET using both client and server"
  },
  {
    "create": "externallauncher.launcher",
    "name": "ts"
  },
  {
    "action": "ts.launch",
    "cmd": "trafficserver",
    "path": "/home/gdb/trafficserver/opt/ts7.1.1/bin/",
    "root_access": true,
    "arguments": [
      "start"
    ],
    "name": "ts1"
  },
  {
    "action": "ts1.scan_for",
    "name": "tsr",
    "keyword": "done"
  },
  {
    "create": "fcgi.client",
    "name": "fcgi"
  },
  {
    "create": "fcgi.server",
    "name": "fs"
  },
  {
    "action": "fs.start"
  },
  {
    "action": "waitfor",
    "event": "fs.ServerStarted",
    "name": "fs1",
    "data": ""
  },
  {
    "action": "parallel",
    "list": [
      [
        {
          "action": "fcgi.request",
          "url": "http://localhost:9080/ajaxExample.php",
          "name": "request1"
        },
        {
          "action": "request1.get",
          "url": "http://localhost:9080/ajaxExample.php",
          "headers": {
            "Connection": "keep-alive"
          },
          "name": "resp1"
        },
        {
          "action": "execute",
          "execute": "print(resp1.headers)"
        },
        {
          "action": "expect",
          "value": "resp1.response_headers.:status",
          "expected": 200,
          "Description": "Request1 Status should be 200 OK"
        },
        {
          "action": "expect",
          "value": "resp1.data",
          "expected": "HelloWorld",
          "Description": "Request1 Data should be same"
        },
        {
          "action": "fcgi.request",
          "url": "http://localhost:9080/ajaxExample.php",
          "name": "request2"
        },
        {
          "action": "request2.get",
          "url": "http://localhost:9080/ajaxExample.php",
          "headers": {
            "Connection": "keep-alive"
          },
          "name": "resp2"
        },
        {
          "action": "expect",
          "value": "resp2.response_headers.:status",
          "expected": 200,
          "Description": "Request2 Status should be 200 OK"
        },
        {
          "action": "expect",
          "value": "resp2.data",
          "expected": "HelloWorld",
          "Description": "Request2 Data should be same"
        }
      ],
      [
        {
          "action": "waitfor",
          "event": "fs.FCGI_STDIN",
          "name": "fsrequest1",
          "data": ""
        },
        {
          "action": "fsrequest1.send_record",
          "request_id": 1,
          "type": "FCGI_STDOUT",
          "data": "Content-type: text/html\r\nContent-Length: 10\r\nCache-Control: max-age=60\r\n\r\nHelloWorld\r\n\r\n",
          "padding_length": 0,
          "version": 1,
          "name": "rec1"
        },
        {
          "action": "fsrequest1.send_record",
          "request_id": 1,
          "type": "FCGI_END_REQUEST",
          "name": "rec2"
        },
        {
          "action": "fs.kill",
          "name": "fsk"
        }
      ]
    ]
  },
  {
    "action": "ts.launch",
    "cmd": "trafficserver",
    "path": "/home/gdb/trafficserver/opt/ts7.1.1/bin/",
    "root_access": true,
    "arguments": [
      "stop"
    ],
    "name": "ts2"
  },
  {
    "action": "ts2.scan_for",
    "name": "tsr",
    "keyword": "done"
  }
]
