[
  {
    "TestName": "FCGI client GET",
    "Description": "Simple fcgi test which starts ATS and php-fastcgi"
  },
  {
    "create": "externallauncher.launcher",
    "name": "php"
  },
  {
    "action": "php.launch",
    "cmd": "php-fastcgi.sh",
    "path": "/home/gdb/trafficserver/AtsFastcgi/InterceptCPPAPI/bin/",
    "arguments": [
      "start"
    ],
    "name": "php1"
  },
  {
    "create": "externallauncher.launcher",
    "name": "ts"
  },
  {
    "action": "ts.launch",
    "cmd": "trafficserver",
    "path": "/home/gdb/trafficserver/opt/ts7.1.1/bin/",
    "root_access": true,
    "arguments": [
      "start"
    ],
    "name": "ts1"
  },
  {
    "action": "ts1.scan_for",
    "name": "tsr",
    "keyword": "done"
  },
  {
    "create": "fcgi.client",
    "name": "fcgi"
  },
  {
    "action": "fcgi.request",
    "url": "http://localhost:9080/html/ajaxExample.php",
    "name": "request1"
  },
  {
    "action": "request1.get",
    "url": "http://localhost:9080/html/ajaxExample.php",
    "headers": {
      "Connection": "keep-alive"
    },
    "name": "resp1"
  },
  {
    "action": "execute",
    "execute": "print(resp1.data);"
  },
  {
    "action": "expect",
    "value": "resp1.response_headers.:status",
    "expected": 200,
    "Description": "Status should be 200 OK"
  },
  {
    "action": "ts.launch",
    "cmd": "trafficserver",
    "path": "/home/gdb/trafficserver/opt/ts7.1.1/bin/",
    "root_access": true,
    "arguments": [
      "stop"
    ],
    "name": "ts2"
  },
  {
    "action": "ts2.scan_for",
    "name": "tsr",
    "keyword": "done"
  },
  {
    "action": "php.launch",
    "cmd": "php-fastcgi.sh",
    "path": "/home/gdb/trafficserver/AtsFastcgi/InterceptCPPAPI/bin/",
    "arguments": [
      "stop"
    ],
    "name": "php2"
  },
  {
    "action": "php2.scan_for",
    "name": "phpend",
    "keyword": "php-cgi"
  }
]
