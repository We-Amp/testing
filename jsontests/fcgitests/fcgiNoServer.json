[
  {
    "TestName": "No FCGI Server and Client GET",
    "Description": "Test fcgi GET using just client with no server"
  },
  {
    "create": "externallauncher.launcher",
    "name": "ts"
  },
  {
    "action": "ts.launch",
    "cmd": "trafficserver",
    "path": "/root/bin/ts-7.1.2_rc/bin",
    "root_access": true,
    "arguments": [
      "start"
    ],
    "name": "ts1"
  },
  {
    "action": "ts1.scan_for",
    "name": "tsr",
    "keyword": "OK"
  },
  {
    "create": "fcgi.client",
    "name": "fcgi"
  },
  {
    "action": "fcgi.request",
    "url": "http://localhost:9080/ajaxExample.php",
    "name": "request1"
  },
  {
    "action": "request1.get",
    "url": "http://localhost:9080/ajaxExample.php",
    "headers": {
      "Connection": "keep-alive"
    },
    "name": "resp1"
  },
  {
    "action": "execute",
    "execute": "print(resp1.headers)"
  },
  {
    "action": "expect",
    "value": "resp1.response_headers.:status",
    "expected": 500,
    "Description": "Status should be 500 Internal Server Error"
  },
  {
    "action": "ts.launch",
    "cmd": "trafficserver",
    "path": "/root/bin/ts-7.1.2_rc/bin",
    "root_access": true,
    "arguments": [
      "stop"
    ],
    "name": "ts2"
  },
  {
    "action": "ts2.scan_for",
    "name": "tsr",
    "keyword": "OK"
  }
]
