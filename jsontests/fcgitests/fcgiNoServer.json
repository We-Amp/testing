[
  {
    "TestName": "No FCGI Server and Client GET",
    "Description": "Test fcgi GET using just client with no server"
  },
  {
    "create": "externallauncher.launcher",
    "name": "ts"
  },
  {
    "action": "ts.launch",
    "cmd": "bin/trafficserver",
    "env_path": "ATS_EXEC",
    "root_access": true,
    "arguments": [
      "start"
    ],
    "name": "ts1"
  },
  {
    "action": "ts1.scan_for",
    "name": "tsr",
    "keyword": "done"
  },
  {
    "create": "http1.client",
    "name": "fcgi"
  },
  {
    "action": "fcgi.get",
    "url": "http://localhost:9080/ajaxExample.php",
    "headers": {
      "Connection": "keep-alive"
    },
    "name": "resp1"
  },
  {
    "action": "execute",
    "execute": "print(resp1.headers)"
  },
  {
    "action": "expect",
    "value": "resp1.response_headers.:status",
    "expected": 500,
    "Description": "Status should be 500 Internal Server Error"
  },
  {
    "action": "ts.launch",
    "cmd": "bin/trafficserver",
    "env_path": "ATS_EXEC",
    "root_access": true,
    "arguments": [
      "stop"
    ],
    "name": "ts2"
  },
  {
    "action": "ts2.scan_for",
    "name": "tsr",
    "keyword": "done"
  }
]
