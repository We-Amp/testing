[
  {
    "TestName": "FCGI client",
    "Description": "This test creates basic fcgi client and starts fcgi server"
  },
  {
    "create": "externallauncher.launcher",
    "name": "php"
  },
  {
    "action": "php.launch",
    "cmd": "php-fastcgi.sh",
    "path": "/home/gdb/trafficserver/AtsFastcgi/InterceptCPPAPI/bin/",
    "arguments": [
      "start"
    ],
    "name": "php1"
  },
  {
    "create": "externallauncher.launcher",
    "name": "ts"
  },
  {
    "action": "ts.launch",
    "cmd": "trafficserver",
    "path": "/home/gdb/trafficserver/opt/ts7.1.1/bin/",
    "root_access": true,
    "password": "password",
    "arguments": [
      "start"
    ],
    "name": "ts1"
  },
  {
    "action": "waitfor",
    "event": "ts.ProcessStarted",
    "name": "tsstarted",
    "data": ""
  },
  {
    "create": "fcgi.client",
    "name": "fcgi"
  },
  {
    "action": "fcgi.request",
    "url": "http://localhost:9080/ajaxExample.php",
    "name": "request1"
  },
  {
    "action": "request1.get",
    "url": "http://localhost:9080/ajaxExample.php",
    "headers": {
      "Connection": "keep-alive"
    },
    "name": "resp1"
  },
  {
    "action": "execute",
    "execute": "print(resp1);"
  },
  {
    "action": "expect",
    "value": "resp1.response_headers.:status",
    "expected": 200,
    "Description": "Status should be 200 OK"
  },
  {
    "action": "php.launch",
    "cmd": "php-fastcgi.sh",
    "path": "/home/gdb/trafficserver/AtsFastcgi/InterceptCPPAPI/bin/",
    "arguments": [
      "stop"
    ],
    "name": "php2"
  },
  {
    "action": "ts.launch",
    "cmd": "trafficserver",
    "path": "/home/gdb/trafficserver/opt/ts7.1.1/bin/",
    "root_access": true,
    "password": "password",
    "arguments": [
      "stop"
    ],
    "name": "ts2"
  }
]
